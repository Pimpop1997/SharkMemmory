‡∏î‡∏π‡∏≠‡∏±‡∏ô‡∏ô‡∏µ‡πâ‡∏ô‡∏¥ ‡∏°‡∏±‡∏ô‡∏û‡∏±‡∏ö‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ

  function line(msg) {
    const timestamp = new Date().toLocaleTimeString();
    logEl.textContent += `[${timestamp}] ${msg}\n`;
    logEl.scrollTop = logEl.scrollHeight;
  }

  // Input handling
  sendBtn.onclick = () => {
    const text = inputEl.value.trim();
    if (text) {
      line(`Input: ${text}`);
      try {
        const result = eval(text);
        line(`Result: ${result}`);
      } catch (err) {
        line(`Error: ${err.message}`);
      }
      inputEl.value = "";
    }
  };

  // Add Enter key support
  inputEl.addEventListener("keypress", (e) => {
    if (e.key === "Enter" && !e.shiftKey) {
      e.preventDefault();
      sendBtn.click();
    }
  });

  window.sharkToolLog = line;
  const buttonsDiv = el.querySelector('.buttons');
  const resetBtn = document.createElement("button");
  resetBtn.id = "resetBtn";
  resetBtn.textContent = "‚ü≤";
  resetBtn.title = "‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á";
  Object.assign(resetBtn.style, {
    color: "#00ffff",
    background: "none",
    border: "none",
    fontWeight: "bold",
    fontSize: "16px",
    cursor: "pointer",
  });
  buttonsDiv.appendChild(resetBtn);

  document.createTextInput = function (options) {
    const input = document.createElement("input");
    input.type = "text";
    input.placeholder = options.placeholder || "";
    input.value = options.value || "";
    return input;
  };
  // Minimize - Fixed
  let minimized = false;
  const minBtn = el.querySelector("#stm-min");
  const bodyEl = el.querySelector(".body");

  minBtn.onclick = () => {
    minimized = !minimized;
    bodyEl.style.display = minimized ? "none" : "flex";
    if (minimized) {
      el.style.height = 'auto';
      el.style.minHeight = 'auto';
    } else {
      el.style.minHeight = '100px';
    }
  };

  // Close
  el.querySelector("#stm-close").onclick = () => {
    el.remove();
    style.remove();
  };
  // Content container
  const content = document.createElement("div");
  content.id = "consoleContent";
  content.style.display = "flex";
  content.style.flexDirection = "column";
  content.style.height = "calc(100% - 40px)";
  consolePanel.appendChild(content);

  // Snippets section
  const snippetsSection = document.createElement("div");
  snippetsSection.style.padding = "12px";
  snippetsSection.style.borderBottom = "1px solid #00ff41";
  snippetsSection.style.display = "flex";
  snippetsSection.style.gap = "8px";
  snippetsSection.style.flexWrap = "wrap";
  snippetsSection.style.overflowX = "auto";
  content.appendChild(snippetsSection);

  function createSnippetButton(label, code) {
    const btn = document.createElement("button");
    btn.textContent = label;
    Object.assign(btn.style, {
      background: "linear-gradient(135deg, #003300, #006600)",
      border: "1px solid #00ff41",
      color: "#00ff41",
      padding: "8px 16px",
      borderRadius: "4px",
      fontWeight: "bold",
      cursor: "pointer",
    });
    // Insert code into the user's input field only ‚Äî do not execute automatically
    btn.addEventListener("click", () => {
      // use the textarea alias and focus/select so user can inspect or run manually
      inputEl.value = code;
      inputEl.focus();
      inputEl.select();
    });
    snippetsSection.appendChild(btn);
  }

  createSnippetButton("Snippet 1", `console.log('Hello from Snippet 1');`);
  createSnippetButton("Snippet 2", `alert('This is Snippet 2');`);

  // Replace old dragging implementation with pointer-based (jspanel-like) dragging

  // ensure header exists
  const header = el.querySelector("header");

  // make header and panel touch-friendly
  header.style.touchAction = "none";

  let draggingPointerId = null;
  let startX = 0,
    startY = 0,
    startLeft = 0,
    startTop = 0;

  // pointer down: start dragging
  header.addEventListener("pointerdown", (e) => {
    // only left button (mouse) or touch/stylus
    if (e.pointerType === "mouse" && e.button !== 0) return;

    header.setPointerCapture(e.pointerId);
    draggingPointerId = e.pointerId;

    // compute current pos and switch to left/top coordinates
    const rect = el.getBoundingClientRect();
    el.style.left = rect.left + "px";
    el.style.top = rect.top + "px";
    el.style.transform = "";
    el.style.right = "auto";
    el.style.bottom = "auto";

    startX = e.clientX;
    startY = e.clientY;
    startLeft = rect.left;
    startTop = rect.top;

    document.body.style.userSelect = "none";
  });

  // pointer move: update position
  document.addEventListener("pointermove", (e) => {
    if (draggingPointerId !== e.pointerId) return;
    e.preventDefault();

    const dx = e.clientX - startX;
    const dy = e.clientY - startY;
    let newLeft = startLeft + dx;
    let newTop = startTop + dy;

    // clamp to viewport with small margins (allow partial offscreen)
    const vw = Math.max(document.documentElement.clientWidth, window.innerWidth || 0);
    const vh = Math.max(document.documentElement.clientHeight, window.innerHeight || 0);
    const elRect = el.getBoundingClientRect();
    // allow ~50px peeking if needed, and keep top >= 0
    const minLeft = -elRect.width + 50;
    const maxLeft = vw - 50;
    const minTop = 0;
    const maxTop = vh - 30;

    newLeft = Math.min(Math.max(newLeft, minLeft), maxLeft);
    newTop = Math.min(Math.max(newTop, minTop), maxTop);

    el.style.left = Math.round(newLeft) + "px";
    el.style.top = Math.round(newTop) + "px";
  });

  // pointer up / cancel: stop dragging
  function endDrag(e) {
    if (draggingPointerId !== e.pointerId) return;
    try {
      header.releasePointerCapture(e.pointerId);
    } catch (err) {}
    draggingPointerId = null;
    document.body.style.userSelect = "";
  }
  document.addEventListener("pointerup", endDrag);
  document.addEventListener("pointercancel", endDrag);

  // reset button: restore to original right/bottom anchored position
  resetBtn.onclick = () => {
    el.style.left = "";
    el.style.top = "";
    el.style.right = "10px";
    el.style.bottom = "10px";
    el.style.transform = "";
    // ensure body visible if minimized
    bodyEl.style.display = minimized ? "none" : "flex";
  };
})();
